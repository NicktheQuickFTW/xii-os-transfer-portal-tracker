# Server Configuration
PORT=3001
NODE_ENV=development

# Database Configuration
DB_HOST=your-project.supabase.co
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your-db-password
DB_NAME=postgres
DB_CA_CERT=your-ca-certificate

# Supabase API Configuration
